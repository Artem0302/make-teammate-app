{
  "name": "makeTeammate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "packager": "react-native start --reset-cache",
    "pods": "cd ios && pod install",
    "android": "react-native run-android",
    "android-release": "react-native run-android --mode=release",
    "ios": "npx react-native run-ios --mode Debug",
    "ios-15-pro": "react-native run-ios --simulator='iPhone 15 Pro'",
    "ios-release": "npx react-native run-ios --mode Release",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint src --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "react-native start",
    "test": "jest --passWithNoTests",
    "prepare": "husky install",
    "add-pre-commit": "npx husky add .husky/prepare-commit-msg 'npx jira-prepare-commit-msg $1'",
    "format": "prettier ./src --write",
    "clean-android": "cd android && ./gradlew clean"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "^5.0.0",
    "react": "18.2.0",
    "react-native": "0.73.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@tsconfig/react-native": "^3.0.3",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.64",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "husky": "^9.0.11",
    "jest": "^29.6.3",
    "jira-prepare-commit-msg": "^1.7.2",
    "lint-staged": "^15.2.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "*.{ts,json,tsx}": "npm run format",
    "*.{ts,tsx}": "npm run lint:fix"
  }
}
